"*":
  "activate-power-mode":
    comboMode:
      activationThreshold: 1
    particles:
      colours:
        fixed: "#fec129"
        type: "random"
      size: {}
      spawnCount:
        max: 25
        min: 10
      totalCount: {}
    screenShake:
      enabled: false
  "atom-beautify":
    css:
      align_assignments: true
      beautify_on_save: true
      end_with_newline: true
      force_indentation: true
    general:
      _analyticsUserId: "83de4dc9-af80-4ff4-90a9-bf76bf029996"
    html:
      beautify_on_save: true
      end_with_newline: true
      indent_inner_html: true
    js:
      beautify_on_save: true
      end_with_newline: true
      jslint_happy: true
  "atom-material-ui":
    fonts: {}
    tabs:
      compactTabs: true
      noTabMinWidth: true
    treeView:
      compactList: true
    ui: {}
  "atom-ternjs":
    inlineFnCompletionDocumentation: true
    snippetsFirst: true
    useSnippetsAndFunction: true
  core:
    disabledPackages: [
      "language-babel"
      "language-vue-component"
      "jsformat"
      "esformatter"
      "vue-snippets"
    ]
    telemetryConsent: "limited"
    themes: [
      "atom-material-ui"
      "atom-material-syntax"
    ]
  editor:
    fontSize: 13
    lineHeight: 1.4
    preferredLineLength: 90
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    zoomFontWhenCtrlScrolling: true
  emmet: {}
  "exception-reporting":
    userId: "d90f9560-aaf7-293a-c1f5-609da6d234d0"
  minimap:
    absoluteMode: true
    adjustAbsoluteModeHeight: true
  welcome:
    showOnStartup: false
".diff":
  editor:
    preferredLineLength: 90
